<?xml version="1.0" encoding="utf-8"?>
<!-- Список внутриигровых триггеров -->
<triggersDict>
	<!-- Описание триггера содержит следующие параметры:
		id={строковое значение} - уникальный идентификатор триггера
		computable=["true","false"] - признак вычислимого триггера, значение которого расчитывается
		на основании текущего состояния, не может быть изменен, не сохраняется в состояниях игры. -->
	<!-- Количество сотрудников указанной професии, количество всех сотрудников, если профессия не указана -->
	<triggerDesc id="employeesCount" computable="true"/>
	<!-- Количество ресурсов указанного типа, количество всех ресурсов, если тип не указан -->
	<triggerDesc id="resourcesCount" computable="true"/>
	<!-- Исследование палки завершено -->
	<triggerDesc id="cudgelResearchComplete"/>
	<!-- Исследование гвоздя завершено -->
	<triggerDesc id="nailResearchComplete"/>
	<!-- Исследование палицы завершено -->
	<triggerDesc id="maceResearchComplete"/>
	<!-- Найдена палка -->
	<triggerDesc id="foundCudgel"/>
	<!-- Найден гвоздь -->
	<triggerDesc id="foundNail"/>
</triggersDict>