<?xml version="1.0" encoding="utf-8"?>
<!-- Список внутриигровых вычислимых триггеров -->
<triggersDict>
	<!-- Описание триггера содержит следующие параметры:
		id={строковое значение} - уникальный идентификатор триггера. -->
	<!-- Количество сотрудников указанной професии на указанной базе, количество всех сотрудников на базе, если профессия не указана,
	количество сотрудников указанной профессии на всех базах, если база не указана -->
	<triggerDesc id="employeesCount"/>
	<!-- Количество ресурсов указанного типа, количество всех ресурсов, если тип не указан -->
	<triggerDesc id="resourcesCount"/>
	<!-- Количество баз у игрока -->
	<triggerDesc id="basesCount"/>
	<!-- Количество известных вражеских баз -->
	<triggerDesc id="targetsCount"/>
	<!-- Количество юнитов на указанной базе, количество всех юнитов, если база не указана -->
	<triggerDesc id="unitsCount"/>
	<!-- Количество указанных модулей на указанной базе, количество всех модулей, если тип модуля не указан,
	количество указанных модулей на всех базах, если база не указана -->
	<triggerDesc id="modulesCount"/>
	<!-- Количество рейдов - удачных (1), неудачных (2), активных (4) (флаги могут быть скомбинированы),
	если флагов нет, возвращается общее количество рейдов -->
	<triggerDesc id="raidsCount"/>
</triggersDict>